
@{
    ViewBag.Title = "DistContacts";
    Layout = "~/Views/Shared/ContentLayout.cshtml";
}
<link href="@Url.Content("~/css/Theadscroll.css")" rel="stylesheet" />
<meta name="viewport" content="width=device-width, initial-scale=1.0">
@{
    var CssStyle = @"@media only screen and (max-width:760px),(min-device-width:768px) and (max-device-width:1024px){.dytable table,.dytable tbody,.dytable td,.dytable th,.dytable thead,.dytable tr{display:block} .dytable thead tr{position:absolute;top:-9999px;left:-9999px}.dytable tr{border:1px solid #ccc}.dytable td{border:none;border-bottom:1px solid #eee;position:relative;padding-left:50%}.dytable td:before{position:absolute;top:6px;left:6px;width:45%;padding-right:10px;white-space:nowrap}.dytable tr td:nth-child(1){width:auto !important;text-align:left !important;}.dytable tr td:nth-child(3){width:auto !important;}.dytable tr td:nth-child(4){width:auto !important;}.dytable tr td:nth-child(10){width:auto !important;}
                    .dytable tr td:nth-of-type(1):before{content:'Dir Seq Cd';font-weight: bold;}
                    .dytable tr td:nth-of-type(2):before{content:'First Name';font-weight: bold;}
                    .dytable tr td:nth-of-type(3):before{content:'Last Name';font-weight: bold;}
                    .dytable tr td:nth-of-type(4):before{content:'Title Code';font-weight: bold;}
                    .dytable tr td:nth-of-type(5):before{content:'Title';font-weight: bold;}
                    .dytable tr td:nth-of-type(6):before{content:'Phone';font-weight: bold;}
                  @media only screen and (min-device-width :320px) and (max-device-width :480px){body{padding:0;margin:0;width:100%}}@media only screen and (min-device-width:768px) and (max-device-width:1024px){body{width:100%}}";
}

<style type="text/css">
        @Html.Raw(CssStyle)
</style>
<script type="text/javascript">
    var DistNo; var rowIndex = 0, Contactno = "";
    var Distname = ""; var ContHeadTitle = "";
    var DContactArray = [];
    var DistContNo;
    var tablename = "", cloudlvl="";

    $(document).ready(function () {
        DisCont = new viewModel();
        ko.applyBindings(DisCont);
        $('#TbDistContact tbody tr:first').addClass('success selected');
       
        $("#btnAdd").click({ handler: DisCont.Add });
        $("#btnEdit").click({ handler: DisCont.Edit });
        $("#btnSave").click({ handler: DisCont.Save });
        $("#btnDelete").click({ handler: DisCont.Delete });
        $("#btnCancel").click({ handler: DisCont.Cancel });

        //For IE
        var isIE = !!navigator.userAgent.match(/Trident/g) || !!navigator.userAgent.match(/MSIE/g);
        if (isIE) {
            $('#TbDistContact').css('height', $(window).height() - 520 + 'px');
            $('table#TbDistContact tbody').css("overflow-y", "scroll");
            $('table#TbDistContact thead').css("width", "98.5%");
        }

    });

    function viewModel() {
        var self = this;

        self.EnableDisableMode = ko.observable(true);
        self.AddMode = ko.observable(false);
        self.EditMode = ko.observable(true);
        self.SaveMode = ko.observable(false);
        //self.DistNo = ko.observable();

        self.DistContNo = ko.observable();
        self.nsalut = ko.observable();
        self.nfirst = ko.observable();
        self.minitial = ko.observable();
        self.nlast = ko.observable();
        self.ntitle = ko.observable();
        self.nseqcd = ko.observable();
        self.ngreet = ko.observable();
        self.ntitlecd = ko.observable();
        self.codea = ko.observable();
        self.newsltr = ko.observable(false);
        self.naddress = ko.observable();
        self.ncity = ko.observable();
        self.nstate = ko.observable();
        self.nzip = ko.observable();
        self.country = ko.observable();
        self.nphone = ko.observable();
        self.nphone2 = ko.observable();
        self.n_ext = ko.observable();
        self.voicemail = ko.observable();
        self.eextn = ko.observable();
        self.cloudlvl = ko.observable();
        self.nshp_addr = ko.observable();
        self.nshp_city = ko.observable();
        self.nshp_st = ko.observable();
        self.nshp_zip = ko.observable();
        self.nshp_cntry = ko.observable();
        self.nfax = ko.observable();
        self.cell = ko.observable();
        self.nemail = ko.observable();
        self.contactno = ko.observable();
        self.Olddistno = ko.observable();


        self.cmbnsalut = ko.observableArray([]);
        self.cmbtitlecd = ko.observableArray([]);
        self.cmbcountry = ko.observableArray([]);
        self.cmbscountry = ko.observableArray([]);
        self.cmbsharefillLvl = ko.observableArray([]);
        self.GDistNo = ko.observable();
        self.DistContGrid = ko.observable();
        
        self.DContTitle = ko.observable("");

        window.parent.CallEnableTabs();

        DistNo = window.parent.document.getElementById('DistNo').value;
        DistName = window.parent.document.getElementById('company').value;
        ContHeadTitle = "Distributor Contacts For: " + DistNo + " - " + DistName;
        self.DContTitle(ContHeadTitle);
        $('#TbDistContact').css('max-height', $(window).height() - 520 + 'px');
        self.GDistNo(DistNo);


        $.ajax({
            url: "GetLookupList",
            data: { LukpID: "SALUT" },
            type: "POST",
            async: false,
            success: function (response) {
                self.cmbnsalut(response.Items);
            }
        });

        $.ajax({
            url: "GetLookupList",
            data: { LukpID: "CLOUD" },
            type: "POST",
            async: false,
            success: function (response) {
                self.cmbsharefillLvl(response.Items);
            }
        });

        $.ajax({
            url: "GetLookupList",
            data: { LukpID: 'TITLE' },
            type: "POST",
            async: false,
            success: function (response) {
                self.cmbtitlecd(response.Items);
            }
        });

        $.ajax({
            url: "GetLookupList",
            data: { LukpID: 'CNTRY' },
            type: "POST",
            async: false,
            success: function (response) {
                self.cmbcountry(response.Items);
                self.cmbscountry(response.Items);

            }
        });



        $(".clickableRow").live("click", function () {
            if (self.SaveMode() == true && self.AddMode() == true) {
                $('.clickableRow').removeClass('success selected');
                return false;
            }
            else if (self.SaveMode() == true && self.AddMode() == false) {
                $('.clickableRow').removeClass('success selected');
                $('#' + rowIndex).addClass('success selected');
            }
            else{
            rowIndex = $(this).attr("id");
            $('.clickableRow').removeClass('selected');
            $(this).addClass('selected');
            }
        });

        /*********Export function code ***********/

        self.menulist = ko.observableArray([]);

        $(".exportclass").live("contextmenu", function (event) {
            event.preventDefault();
            event.stopPropagation();
            tablename = $(this).closest('table').attr('id');
            $('#export').remove();
            if (event.which == 3) {
                self.menulist().length = 0;
                evecount = event.currentTarget.children;
                for (var i = 0; i < evecount.length; i++) {
                    self.menulist.push(evecount[i].innerText);
                    var clsname = evecount[i].className;
                    var clsindex = clsname.indexOf("hide");
                    if (clsindex >= 0) {
                        $("#chk_" + [i]).attr('checked', false);
                    }
                    else {
                        $("#chk_" + [i]).attr('checked', true);
                    }
                }
                self.menulist();
                $("#NotesDialog").dialog("open");
                $('#ui-id-1').prepend('<img id="export" style="width:20px; height:20px;float:right" src="../images/Export.png" title="Export Data" onclick="fnExcelReport();" />');

            }

        });
        doublesort();
        /*********End Export function code ***********/

        $("#lblemail").live("click", function () {
            send_email(self.nemail());
        });

        
        function FillGrid() {
            $.ajax({
                url: "FillDistContGrid",
                data: { DistnNo: self.GDistNo(), Contactno: Contactno },
                type: "POST",
                async: false,
                success: function (response) {
                    $('#TbDistContact th i').removeClass("icon-white icon-chevron-up icon-white icon-chevron-down");
                    self.DistContGrid(response.Items);
                    if (response.rowindex > 0) {
                        $("#" + response.rowindex).addClass('success selected');
                        self.SelectedDContactList(response.Items[response.rowindex]);
                    }
                    else {
                        $("#" + response.rowindex).addClass('success selected');
                        self.SelectedDContactList(response.Items[0]);
                    }
                    if (response.Items.length == 0) {
                        clearfields();
                        self.contactno("");
                        $("#lblemail").css("color", "#333333");
                        $("#lblemail").css("cursor", "default");
                        $("#lblemail").css("text-decoration", "none");
                    }

                    var THIndex = $('#TbDistContact thead th').length;
                    for (var i = 0; i < THIndex; i++) {
                        
                        var clsname = $('#TbDistContact thead th')[i].className;
                        var clsindex = clsname.indexOf("hide");
                        if (clsindex >= 0) {
                            $('td').filter(':nth-child(' + (i + 1) + ')').addClass('hide');
                        }
                    }
                }
            });
        }

        self.SelectedDContactList = function (item) {
            if (self.SaveMode() == true)
                return false;
            if (item != undefined) {
                self.Olddistno(item.Olddistno)
                self.contactno(item.contactno);
                Contactno = item.contactno;
                DistContNo= item.DistContNo;
                self.nsalut(item.nsalut);
                self.nfirst(item.nfirst);
                self.minitial(item.minitial.trim());
                self.nlast(item.nlast);
                self.ntitle(item.ntitle);
                self.nseqcd(item.nseqcd.trim());
                self.ngreet(item.ngreet);
                self.ntitlecd(item.ntitlecd);
                self.codea(item.codea.trim());
                //self.newsltr(item.newsltr.trim());

                //if (item.newsltr == 'True' || item.newsltr==true)
                //    self.newsltr(true)
                //else
                //    self.newsltr(false)
                if (item.newsltr.trim() == "")
                    self.newsltr(false)
                else
                    self.newsltr(true)
                self.naddress(item.naddress);
                self.ncity(item.ncity);
                self.nstate(item.nstate.trim());
                self.nzip(item.nzip);
                self.country(item.country);
                self.nphone(item.nphone);
                self.nphone2(item.nphone2);
                self.n_ext(item.n_ext);
                self.voicemail(item.voicemail);
                self.eextn(item.eextn);
                self.cloudlvl(item.cloudlvl);
                cloudlvl = item.cloudlvl;
                self.nshp_addr(item.nshp_addr);
                self.nshp_city(item.nshp_city);
                self.nshp_st(item.nshp_st.trim());
                self.nshp_zip(item.nshp_zip);
                self.nshp_cntry(item.nshp_cntry);
                self.nfax(item.nfax);
                self.cell(item.cell);
                self.nemail(item.nemail);

                if (item.nemail != null && item.nemail != "") {
                    $("#lblemail").css("color", "blue");
                    $("#lblemail").css("cursor", "pointer");
                    $("#lblemail").css("text-decoration", "underline");
                }
                else {
                    $("#lblemail").css("color", "#333333");
                    $("#lblemail").css("cursor", "default");
                    $("#lblemail").css("text-decoration", "none");
                }
            }
           
        }

        $("#zipcode").live("blur", function () {
            if (self.SaveMode() == true) {
                if (self.nstate() == "" && self.ncity() == "") {
                    //blurevent = true;
                    if (self.nzip() == null || self.nzip() == "") {
                        return false;
                    }
                    $.ajax({
                        url: "searchzip",
                        data: { getzip: self.nzip() },
                        type: "POST",
                        success: function (response) {
                            if (response.Items.length > 0) {
                                $("#city").val(response.Items[0].City);
                                $("#state").val(response.Items[0].State);
                                self.ncity($("#city").val());
                                self.nstate($("#state").val());
                            }
                            else {
                                $("#city").val("");
                                $("#state").val("");
                                self.ncity($("#city").val());
                                self.nstate($("#state").val());
                            }

                        }
                    });
                }
                else {
                    return false;
                }
            }
        });

        $("#seczipcode").live("blur", function () {
            if (self.SaveMode() == true) {
                if (self.nshp_st() == "" && self.nshp_city() == "") {
                   
                    if (self.nshp_zip() == null || self.nshp_zip() == "") {
                        return false;
                    }
                    $.ajax({
                        url: "searchzip",
                        data: { getzip: self.nshp_zip() },
                        type: "POST",
                        success: function (response) {
                            if (response.Items.length > 0) {
                                $("#seccity").val(response.Items[0].City);
                                $("#secstate").val(response.Items[0].State);
                                self.nshp_city() = $("#seccity").val();
                                self.nshp_st() = $("#secstate").val();
                            }
                            else {
                                $("#seccity").val("");
                                $("#secstate").val("");
                                self.nshp_city() = $("#seccity").val();
                                self.nshp_st() == $("#secstate").val();
                            }

                        }
                    });
                }
                else {
                    return false;
                }
            }
        });

        FillGrid();

        function clearfields() {
            self.nsalut("");
            self.nfirst("");
            self.minitial("");
            self.nlast("");
            self.ntitle("");
            self.nseqcd("");
            self.ngreet("");
            self.ntitlecd("");
            self.codea("");
            self.newsltr(false);
            self.naddress("");
            self.ncity("");
            self.nstate("");
            self.nzip("");
            self.country("");
            self.nphone("");
            self.nphone2("")
            self.n_ext("");
            self.eextn("");
            self.nshp_city("");
            self.nfax("");
            self.cell("");
            self.nemail("");
            self.nshp_st("");
            self.nshp_zip("");
            self.nshp_cntry("");
            self.nshp_addr("");
            self.voicemail("");
            self.cloudlvl("");
        }

        self.Add = function () {
            if (self.SaveMode() == true)
                return false;

            window.parent.CallDisableTabs();

            clearfields();

            self.AddMode(true);
            self.SaveMode(true);
            self.EditMode(false);
            self.EnableDisableMode(false);
           
            document.getElementById('nsalut').focus();
            $('.clickableRow').removeClass('success selected');
            $("#lblemail").css("color", "#333333");
            $("#lblemail").css("cursor", "default");
            $("#lblemail").css("text-decoration", "none");
        }
        self.Edit = function () {
            if (self.DistContGrid().length == 0)
                return false;

            window.parent.CallDisableTabs();

            self.AddMode(false);
            self.SaveMode(true);
            self.EditMode(false);
            self.EnableDisableMode(false);
            document.getElementById('nfirst').focus();
        }

        self.Save = function () {

            DContactArray = [];
            DContactArray =
                {
                   
                    contactno: Contactno,
                    DistNo:DistNo,
                    DistContNo: DistContNo,
                    //Olddistno: self.Olddistno(),
                    nsalut: self.nsalut(),
                    nfirst: self.nfirst(),
                    minitial: self.minitial(),
                    nlast: self.nlast(),
                    ntitle: self.ntitle(),
                    nseqcd: self.nseqcd(),
                    ngreet: self.ngreet(),
                    ntitlecd: self.ntitlecd(),
                    codea: self.codea(),
                    newsltr: self.newsltr(),
                    naddress: self.naddress(),
                    ncity: self.ncity(),
                    nstate: self.nstate(),
                    nzip: self.nzip(),
                    country: self.country(),
                    nphone: self.nphone(),
                    nphone2:self.nphone2(),
                    n_ext: self.n_ext(),
                    eextn: self.eextn(),
                    nshp_city: self.nshp_city(),
                    nfax: self.nfax(),
                    cell: self.cell(),
                    //newsltr: self.newsltr(),
                    nemail: self.nemail(),
                    nshp_st: self.nshp_st(),
                    nshp_zip: self.nshp_zip(),
                    nshp_cntry: self.nshp_cntry(),
                    nshp_addr: self.nshp_addr(),
                    voicemail: self.voicemail(),
                    cloudlvl: self.cloudlvl()
                }









            if (self.nfirst() == null || self.nfirst() == "") {
                $.msgBox({
                    title: "Gro Group",
                    content: "Please enter the First Name",
                    type: "info",
                    buttons: [{ value: "Ok" }],
                    afterClose: function (result) {
                        document.getElementById('firstname').focus();
                    }
                });
                return false;
            }
            if (self.nlast() == null || self.nlast() == "") {
                $.msgBox({
                    title: "Gro Group",
                    content: "Please enter the Last Name",
                    type: "info",
                    buttons: [{ value: "Ok" }],
                    afterClose: function (result) {
                        document.getElementById('lastname').focus();
                    }
                });
                return false;
            }
            if (self.nphone() != null && self.nphone() != "") {
                if (self.nphone().length < 12) {
                    $.msgBox({
                        title: "Gro Group",
                        content: "Please enter the Valid Phone no",
                        type: "info",
                        buttons: [{ value: "Ok" }],
                        afterClose: function (result) {
                            document.getElementById('nphone').focus();
                        }
                    });
                    return false;
                }
            }
            if (self.nphone2() != null && self.nphone2() != "") {
                if (self.nphone2().length < 12) {
                    $.msgBox({
                        title: "Gro Group",
                        content: "Please enter the Valid Phone no",
                        type: "info",
                        buttons: [{ value: "Ok" }],
                        afterClose: function (result) {
                            document.getElementById('nphone2').focus();
                        }
                    });
                    return false;
                }
            }
            if (self.nfax() != null && self.nfax() != "") {
                if (self.nfax().length < 12) {
                    $.msgBox({
                        title: "Gro Group",
                        content: "Please enter the Valid Fax no",
                        type: "info",
                        buttons: [{ value: "Ok" }],
                        afterClose: function (result) {
                            document.getElementById('nfax').focus();
                        }
                    });
                    return false;
                }
            }
            
            if (self.cell() != null && self.cell() != "") {
                if (self.cell().length < 12) {
                    $.msgBox({
                        title: "Gro Group",
                        content: "Please enter the Valid Phone no",
                        type: "info",
                        buttons: [{ value: "Ok" }],
                        afterClose: function (result) {
                            document.getElementById('cell').focus();
                        }
                    });
                    return false;
                }
            }

            if (self.nemail() != "" && self.nemail() != null) {
                if (!ValidateEmail(self.nemail())) {
                    $.msgBox({
                        title: "Gro Group", content: "Invalid email",
                        afterClose: function (result) {
                            self.nemail("");
                            document.getElementById('nemail').focus();
                        }
                    });
                    return false;
                }
            }
            
            $.ajax({
                url: "ValidateFirstLastName",
                data: { FirstName: self.nfirst(), LastName: self.nlast(), DistContNo: DistContNo, AddMode: self.AddMode(), DistNo: DistNo, titlecd: self.ntitlecd() },
                type: "POST",
                async: false,
                success: function (response) {
                    if (response.Items == true) {
                        $.msgBox({
                            title: "Gro Group", content: "Name already exists",
                            afterClose: function (result) {
                                return false;
                            }
                        });
                    }
                    else {
                        saveDistContacts();
                    }
                }
            });
            
        }

        function saveDistContacts() {
            $.ajax({
                url: "SaveDistContacts",
                data: { ObjDistCntsArray: JSON.stringify(DContactArray), DistNo: self.GDistNo(), IsAddMode: self.AddMode() },
                type: "POST",
                success: function (response) {
                    if (response.ItemMsg != undefined && response.ItemMsg != null) {
                        alert("Gro Group", response.ItemMsg);
                    } else {
                        Contactno = response.Items;
                        $('#TbDistContact tbody tr').remove();
                        FillGrid();
                       
                        self.contactno(Contactno);
                    }
                }
            });

            window.parent.CallEnableTabs();

            self.SaveMode(false);
            self.AddMode(false);
            self.EditMode(true);
            self.EnableDisableMode(true);
        }
        
        self.Cancel = function () {
            if (self.SaveMode() != true)
                return false;
            alertconfirm("Are You Sure", "Do you want to cancel current changes ?", self.confirmcancel, true, false);
        }

        self.confirmcancel = function () {
            self.AddMode(false);
            self.EditMode(true);
            self.SaveMode(false);
            self.EnableDisableMode(true);

            window.parent.CallEnableTabs();

            if (self.DistContGrid().length > 0) {
                //FillGrid();
                self.SelectedDContactList(self.DistContGrid()[rowIndex]);
            }
            $('#TbDistContact tbody tr').remove();
            FillGrid();
            self.DContTitle(ContHeadTitle);
            $('.clickableRow').removeClass('success selected');
            $("#" + rowIndex).addClass('success selected');
        }

        self.Delete = function () {
            if (self.SaveMode() == true || self.DistContGrid().length == 0)
                return false;
            //alerterror("Denied", "Record cannot be deleted");
            //if (self.SaveMode() == true)
            //    return false;
            alertconfirm("Delete", "Are you sure you want to delete this record ?", self.confirmdelete, false, true);
        }
        self.confirmdelete = function () {
            var name = self.nfirst() + " " + self.nlast();

            $.ajax({
                url: "DeleteDistContacts",
                data: { Distno: DistNo, contactno: self.contactno(), },
                type: "POST",
                success: function (response) {
                    if (response.checkroomlist == false) {
                        $('#TbDistContact tbody tr').remove();
                        FillGrid();
                    }
                    else {
                        alerterror("Denied", "This contact [" + name + "] cannot be deleted. This contact is used in Room List.");
                       // alerterror("Denied", "Record cannot be deleted");
                    }
                   
                }
            });
        }

    }

    //Export function code 

    function fnExcelReport() {
        var table = document.getElementById(tablename);
        GlobalExcelExportsortable(table, "DistContacts", tablename + "_.csv", "Screen", tablename + "_");
    }
</script>

<center>
    <div style="max-width:1200px !important;height:810px;overflow-y:auto;overflow-x:hidden; text-align:left;">
        <div class="row-fluid">
            <div class="span4">
                <ul class="list-nostyle list-inline tooltipsample">
                    <li data-bind="enable:EditMode"><a data-placement="bottom" data-rel="tooltip" data-original-title="Add" title="Add" class="btn btn-primary" id="btnAdd"><i class="icon-file icon-white"></i></a></li>
                    <li data-bind="visible: EditMode"><a data-placement="bottom" data-rel="tooltip" data-original-title="Edit" title="Edit" class="btn btn-warning" id="btnEdit"><i class="icon-edit icon-white"></i></a></li>
                    <li data-bind="visible: SaveMode"><a data-placement="bottom" data-rel="tooltip" data-original-title="Save" title="Save" class="btn btn-warning" id="btnSave"><i class="icon-list-alt icon-white"></i></a></li>
                    <li data-bind="enable:EditMode"><a data-placement="bottom" data-rel="tooltip" data-original-title="Delete" title="Delete" class="btn btn-danger" id="btnDelete"><i class="icon-remove icon-white"></i></a></li>
                    <li data-bind="enable:SaveMode"><a data-placement="bottom" data-rel="tooltip" data-original-title="Cancel" title="Cancel" class="btn btn-info" id="btnCancel"><i class="iconsweets-arrowleft iconsweets-white"></i></a></li>
                </ul>
            </div>
        </div>
        <div class="row-fluid">
            <div class="row-fluid">
                <div class="widgetbox">
                    <h4 class="widgettitle"><span data-bind="text: DContTitle"></span><span style="margin-left:10%">Contact: <span data-bind="text:contactno"></span></span><span style="float:right;margin-right: 10%;">Old DistNo: <span data-bind="text:Olddistno"></span></span></h4>
                    <div class="widgetcontent">
                        <div class="row-fluid">
                            <div class="span12">
                                <div class="row-fluid">
